<!-- a navigation_lib macro -->
<!-- $Id$ -->

<metal:block define-macro="render_listing_header_multi_select_map">
  <input name="uids:list" type="checkbox" class="noborder"
         tal:attributes="value listing_info/current_uid;
                         id listing_info/current_uid" />
  <label tal:content="python: listing_info['current'].get(display_attr, '%s
                      not found' % display_attr)"
         tal:attributes="for listing_info/current_uid" />
</metal:block>
